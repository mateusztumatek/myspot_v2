        # text/html is always compressed by HttpGzipModule
        #text/css
        #text/javascript
        #text/xml
        #text/plain
        #text/x-component
        #application/javascript
        #application/x-javascript
        #application/json
        #application/xml
        #application/rss+xml
        #application/atom+xml
        #font/truetype
        #font/opentype
        #application/vnd.ms-fontobject
        #image/svg+xml;
